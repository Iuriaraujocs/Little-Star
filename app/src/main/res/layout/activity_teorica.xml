<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#f5e9b9"
    tools:context=".activities.TeoricaActivity">

    <ImageView
        android:id="@+id/caoid"
        android:layout_width="150dp"
        android:layout_height="159dp"
        android:layout_alignTop="@+id/imageView12"
        android:layout_alignParentStart="true"
        android:layout_alignParentLeft="true"
        android:layout_alignParentBottom="true"
        android:layout_marginStart="27dp"
        android:layout_marginLeft="27dp"
        android:layout_marginTop="47dp"
        android:layout_marginBottom="375dp"
        android:src="@drawable/cao" />

    <ImageView
        android:id="@+id/gatoid"
        android:layout_width="138dp"
        android:layout_height="134dp"
        android:layout_alignTop="@+id/imageView12"
        android:layout_alignParentEnd="true"
        android:layout_alignParentRight="true"
        android:layout_marginTop="130dp"
        android:layout_marginEnd="57dp"
        android:layout_marginRight="57dp"
        android:src="@drawable/gato" />

    <ImageView
        android:id="@+id/leaoid"
        android:layout_width="134dp"
        android:layout_height="146dp"
        android:layout_alignParentStart="true"
        android:layout_alignParentLeft="true"
        android:layout_alignParentBottom="true"
        android:layout_centerVertical="true"
        android:layout_marginStart="34dp"
        android:layout_marginLeft="34dp"
        android:layout_marginEnd="24dp"
        android:layout_marginRight="24dp"
        android:layout_marginBottom="211dp"
        android:layout_toStartOf="@+id/macacoid"
        android:layout_toLeftOf="@+id/macacoid"
        android:src="@drawable/leao" />

    <ImageView
        android:id="@+id/macacoid"
        android:layout_width="151dp"
        android:layout_height="156dp"
        android:layout_alignParentEnd="true"
        android:layout_alignParentRight="true"
        android:layout_alignParentBottom="true"
        android:layout_marginEnd="68dp"
        android:layout_marginRight="68dp"
        android:layout_marginBottom="213dp"
        android:src="@drawable/macaco" />

    <ImageView
        android:id="@+id/ovelhaid"
        android:layout_width="145dp"
        android:layout_height="123dp"
        android:layout_alignParentStart="true"
        android:layout_alignParentLeft="true"
        android:layout_alignParentBottom="true"
        android:layout_marginStart="42dp"
        android:layout_marginLeft="42dp"
        android:layout_marginBottom="57dp"
        android:src="@drawable/ovelha" />

    <ImageView
        android:id="@+id/vacaid"
        android:layout_width="165dp"
        android:layout_height="wrap_content"
        android:layout_alignParentEnd="true"
        android:layout_alignParentRight="true"
        android:layout_alignParentBottom="true"
        android:layout_marginEnd="59dp"
        android:layout_marginRight="59dp"
        android:layout_marginBottom="62dp"
        android:src="@drawable/vaca" />

    <ImageView
        android:id="@+id/imageView12"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentStart="true"
        android:layout_alignParentLeft="true"
        android:layout_alignParentTop="true"
        android:layout_centerHorizontal="true"
        android:layout_marginStart="74dp"
        android:layout_marginLeft="74dp"
        android:layout_marginTop="28dp"
        android:src="@drawable/logo" />
</RelativeLayout>